{
  "name": "next-latest-starter",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "ci": "payload migrate && pnpm build",
    "generate:types": "payload generate:types",
    "generate:schema": "payload-graphql generate:schema",
    "generate:importmap": "payload generate:importmap"
  },
  "engines": {
    "node": "^18.20.2 || >=20.6.0"
  },
  "dependencies": {
    "@lexical/clipboard": "^0.27.2",
    "@lexical/code": "^0.27.2",
    "@lexical/hashtag": "^0.27.2",
    "@lexical/headless": "^0.27.2",
    "@lexical/html": "^0.27.2",
    "@lexical/link": "^0.27.2",
    "@lexical/list": "^0.27.2",
    "@lexical/mark": "^0.27.2",
    "@lexical/markdown": "^0.27.2",
    "@lexical/overflow": "^0.27.2",
    "@lexical/react": "^0.27.2",
    "@lexical/rich-text": "^0.27.2",
    "@lexical/selection": "^0.27.2",
    "@lexical/table": "^0.27.2",
    "@lexical/utils": "^0.27.2",
    "@payloadcms/db-mongodb": "^3.28.1",
    "@payloadcms/db-postgres": "^3.28.1",
    "@payloadcms/next": "^3.28.1",
    "@payloadcms/ui": "^3.28.1",
    "classnames": "^2.5.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.10.0",
    "json-schema": "^0.4.0",
    "lexical": "^0.27.2",
    "next": "^15.2.2",
    "payload": "3.28.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^5.0.0",
    "sharp": "0.33.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.13.10",
    "@types/react": "19.0.10",
    "@types/react-dom": "19.0.4",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.7",
    "eslint": "^9.22.0",
    "eslint-config-next": "15.2.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
